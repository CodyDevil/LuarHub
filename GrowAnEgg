local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Luar Hub",
    SubTitle = "By Vouxy",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Darker",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "package" }),
    Others = Window:AddTab({ Title = "Others", Icon = "settings-2" }),
    Teleports = Window:AddTab({ Title = "Teleports", Icon = "map" }),
    Credits = Window:AddTab({ Title = "Credits", Icon = "heart" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Options = Fluent.Options

Fluent:Notify({
    Title = "Luar Hub",
    Content = "Script successfully loaded.",
    Duration = 3
})

-- âœ… Select Egg
local eggList = {
    "Chicken Egg", "Crow Egg", "Turtle Egg",
    "Crab Egg", "Duck Egg", "Red Parrot Egg",
    "Billy Egg", "Monkey Egg", "Horse Egg",
    "Penguin Egg", "Storm Snake Egg", "Wild Snake Egg",
    "Stegosaurus Egg", "Gastonia Egg", "Elephant Egg",
    "Alligator Egg", "Blossom Fly Egg", "Leafjaw Egg",
    "Mammoth Egg", "Crocodile Egg", "Sea Monster Egg",
    "Wild Shark Egg", "Dragon Egg", "Orchidra Egg",
    "Venom Dragon Egg", "Dreadmaw Egg"
}

local selectedEgg = eggList[1]

Tabs.Main:AddDropdown("SelectEgg", {
    Title = "Select Egg",
    Values = eggList,
    Multi = false,
    Default = 1
}):OnChanged(function(value)
    selectedEgg = value
end)

Tabs.Main:AddButton({
    Title = "Purchase Egg",
    Callback = function()
        game:GetService("ReplicatedStorage").Remotes.PurchaseEggFunc:InvokeServer(selectedEgg)
    end
})

-- âœ… Select Seed
local seedList = {
    "Instant Grow Seed", "Lost Seed", "Golden Seed", "Wild Seed",
    "Crimson Seed", "Fire Seed", "Shadow Seed", "Neon Seed",
    "Nature Seed", "Deep Seed", "Firework Seed", "Rainbow Seed"
}

local selectedSeed = seedList[1]

Tabs.Main:AddDropdown("SelectSeed", {
    Title = "Select Seed",
    Values = seedList,
    Multi = false,
    Default = 1
}):OnChanged(function(value)
    selectedSeed = value
end)

Tabs.Main:AddButton({
    Title = "Purchase Seed",
    Callback = function()
        game:GetService("ReplicatedStorage").Remotes.PurchaseSeedFunc:InvokeServer(selectedSeed)
    end
})

-- âœ… Sell Inventory
Tabs.Main:AddButton({
    Title = "Sell All Inventory",
    Callback = function()
        game:GetService("ReplicatedStorage").Remotes.SellInventoryFunc:InvokeServer()
    end
})

-- âœ… Speed Boost
local speedSlider = Tabs.Others:AddSlider("SpeedBoost", {
    Title = "Speed Boost",
    Description = "Adjust walk speed.",
    Default = 16,
    Min = 16,
    Max = 100,
    Rounding = 1
})

local boosting = true
task.spawn(function()
    while true do
        task.wait(0.1)
        if boosting then
            local human = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            if human then
                human.WalkSpeed = speedSlider.Value
            end
        end
        if Fluent.Unloaded then break end
    end
end)

-- âœ… Claim Rewards
Tabs.Others:AddButton({
    Title = "Claim Rewards",
    Callback = function()
        for i = 1, 7 do
            game:GetService("ReplicatedStorage").Remotes.TimeRewardFunc:InvokeServer(i)
            task.wait(0.2)
        end
    end
})

-- âœ… Infinity Yield
Tabs.Others:AddButton({
    Title = "Infinity Yield",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end
})

-- âœ… Auto Spin
local autoSpin = false

Tabs.Others:AddToggle("AutoSpin", {
    Title = "Auto Spin",
    Default = false
}):OnChanged(function(value)
    autoSpin = value
    if autoSpin then
        task.spawn(function()
            while autoSpin do
                game:GetService("ReplicatedStorage").Events.SpinRequest:FireServer()
                task.wait(0.5)
            end
        end)
    end
end)

-- âœ… Teleports
local teleportPoints = {
    ["Egg Store"] = Vector3.new(-122.4, 3.5, -119.2),
    ["Sell Store"] = Vector3.new(-88.8, 3.5, -121.5),
    ["Craft Store"] = Vector3.new(-109.5, 5.3, 57.0),
    ["Seed Store"] = Vector3.new(-107.6, 3.1, 244.7),
    ["Reviewer"] = Vector3.new(-105.6, 3.5, -125.3)
}

local teleportNames = {}
for name, _ in pairs(teleportPoints) do
    table.insert(teleportNames, name)
end

table.sort(teleportNames)

local selectedLocation = teleportNames[1]

Tabs.Teleports:AddDropdown("TeleportList", {
    Title = "Select Location",
    Values = teleportNames,
    Multi = false,
    Default = 1
}):OnChanged(function(value)
    selectedLocation = value
end)

Tabs.Teleports:AddButton({
    Title = "Teleport",
    Callback = function()
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            char.HumanoidRootPart.CFrame = CFrame.new(teleportPoints[selectedLocation])
        end
    end
})

-- âœ… Credits
Tabs.Credits:AddParagraph({
    Title = "Credits",
    Content = "Scriptblox: Vouxy\nDiscord: shiergy\nGithub: CodyDevil"
})

-- ðŸ§© Fluent Addons
SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)
SaveManager:IgnoreThemeSettings()
SaveManager:SetIgnoreIndexes({})
InterfaceManager:SetFolder("FluentScriptHub")
SaveManager:SetFolder("FluentScriptHub/specific-game")
InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)

Window:SelectTab(1)
SaveManager:LoadAutoloadConfig()
